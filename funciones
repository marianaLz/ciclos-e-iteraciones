function addNumbers(numberOne, numberTwo) {
  return numberOne + numberTwo;
}
var resultAdd = addNumbers(10,5);
console.log("The result is " + resultAdd);

function substractNumbers(numberOne, numberTwo) {
  return numberOne - numberTwo;
}
var resultSubs = substractNumbers(10,5);
console.log("The result is " + resultSubs);

function multiplyNumbers(numberOne, numberTwo) {
  return numberOne * numberTwo;
}
var resultMult = multiplyNumbers(10,5);
console.log("The result is " + resultMult);

function divideNumbers(numberOne, numberTwo) {
  return numberOne / numberTwo;
}
var resultDiv = divideNumbers(10,5);
console.log("The result is " + resultDiv);

function calculator(numberOne, numberTwo, operation) {
  if(operation === "sum"){
    return numberOne + numberTwo;
  }
  else if(operation === "subs"){
    return numberOne - numberTwo;
  }
  else if(operation === "multiply"){
    return numberOne * numberTwo;
  }
  else if(operation === "div"){
    return numberOne / numberTwo;
  }
}
var resultCalculator = calculator(10,5,"sum");
console.log("The result is " + resultCalculator);
